{
    "name": "rougin/combustor",
    "description": "Generate controllers, models, and views from database for CodeIgniter",
    "keywords": [
        "codeigniter",
        "combustor",
        "crud",
        "generator",
        "php"
    ],
    "homepage": "https://github.com/rougin/combustor",
    "license": "MIT",
    "authors": [
        {
            "name": "Rougin Royce Gutib",
            "email": "rougingutib@gmail.com",
            "homepage": "http://rougin.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "rougin/blueprint": "dev-master",
        "rougin/describe": "dev-master",
        "rougin/spark-plug": "~0.4.2"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "rougin/codeigniter": "~3.0",
        "rougin/credo": "dev-master",
        "rougin/wildfire": "dev-master",
        "scrutinizer/ocular": "~1.1"
    },
    "bin": [
        "bin/combustor",
        "bin/combustor.php"
    ],
    "autoload": {
        "psr-4": {
            "Rougin\\Combustor\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rougin\\Combustor\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    },
    "suggest": {
        "rougin/codeigniter": "Installs CodeIgniter core through Composer",
        "rougin/refinery": "\"Ready-to-eat\" migrations for Codeigniter"
    }
}
